<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="312.0" prefWidth="518.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lt.markmerkk.ui.clock.ClockPresenter">
   <children>
      <HBox>
         <children>
            <Button fx:id="buttonRefresh" minHeight="38.0" minWidth="38.0" mnemonicParsing="false" onAction="#onClickSearch" stylesheets="@../../../../refresh.css">
               <HBox.margin>
                  <Insets left="5.0" top="5.0" />
               </HBox.margin>
               <graphic>
                  <ProgressIndicator fx:id="taskLoadIndicator" minHeight="20.0" minWidth="20.0" onMouseClicked="#onClickSearch" prefHeight="20.0" prefWidth="20.0" visible="false" />
               </graphic>
            </Button>
            <VBox fx:id="testContainer" HBox.hgrow="ALWAYS">
               <children>
                  <TextField fx:id="inputTask" minHeight="30.0" />
                  <Text fx:id="outputJQL" fill="#aeaeae" strokeType="OUTSIDE" strokeWidth="0.0" text="JQL: ">
                     <font>
                        <Font size="9.0" />
                     </font>
                  </Text>
               </children>
               <HBox.margin>
                  <Insets left="5.0" top="5.0" />
               </HBox.margin>
            </VBox>
            <Button fx:id="buttonOpen" minHeight="38.0" minWidth="38.0" mnemonicParsing="false" onAction="#onClickOpen" stylesheets="@../../../../open.css">
               <HBox.margin>
                  <Insets left="5.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="buttonSettings" minHeight="38.0" minWidth="38.0" mnemonicParsing="false" onAction="#onClickSettings" stylesheets="@../../../../settings.css">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308">
         <children>
            <ToggleButton fx:id="buttonClock" minHeight="28.0" mnemonicParsing="false" onAction="#onClickClock" stylesheets="@../../../../play_pause.css" />
            <DatePicker fx:id="inputFrom" minHeight="28.0" minWidth="160.0" promptText="From" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </DatePicker>
            <DatePicker fx:id="inputTo" minHeight="28.0" minWidth="160.0" promptText="To" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </DatePicker>
            <Button fx:id="buttonEnter" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="28.0" mnemonicParsing="false" onAction="#onClickEnter" stylesheets="@../../../../enter.css" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </VBox.margin>
      </HBox>
      <TextArea fx:id="inputComment" prefHeight="300.0" prefWidth="200.0" promptText="Comment" wrapText="true" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </VBox.margin>
      </TextArea>
      <HBox VBox.vgrow="ALWAYS" />
   </children>
</VBox>
