<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.paint.*?>
<?import javafx.geometry.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-stylesheet /css/material.css?>

<BorderPane fx:id="jfxRoot" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lt.markmerkk.ui_2.MainPresenter2">
   <left>
      <VBox alignment="TOP_CENTER" minHeight="70.0" minWidth="70.0" style="-fx-background-color: #5C6BC0;" BorderPane.alignment="CENTER">
         <children>
            <StackPane>
               <children>
                  <JFXToggleNode fx:id="jfxToggleClock" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" style="-fx-background-radius: 100;" styleClass="toggle-icon1">
                     <graphic>
                        <JFXButton fx:id="jfxButtonClock" alignment="CENTER" ellipsisString="..." maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" ripplerFill="WHITE" styleClass="button-default-clock" textFill="WHITE" textOverrun="WORD_ELLIPSIS">
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </JFXButton>
                     </graphic>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </JFXToggleNode>
                  <VBox alignment="TOP_RIGHT" fillWidth="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" pickOnBounds="false" prefHeight="50.0" prefWidth="50.0">
                     <children>
                        <JFXButton fx:id="jfxButtonClockSettings" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="15.0" prefWidth="15.0" ripplerFill="WHITE" styleClass="button-settings-mini" textOverrun="CLIP">
                           <VBox.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </VBox.margin>
                        </JFXButton>
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets top="6.0" />
               </VBox.margin>
            </StackPane>
            <VBox alignment="BOTTOM_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" VBox.vgrow="ALWAYS">
               <children>
                  <JFXButton fx:id="jfxButtonDate" alignment="CENTER" ellipsisString="..." maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" ripplerFill="WHITE" styleClass="button-default-clock" textFill="WHITE" textOverrun="WORD_ELLIPSIS">
                     <VBox.margin>
                        <Insets bottom="6.0" />
                     </VBox.margin>
                  </JFXButton>
                  <JFXButton fx:id="jfxButtonSettings" alignment="CENTER" ellipsisString="..." maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" ripplerFill="WHITE" styleClass="button-default-clock" textFill="WHITE" textOverrun="WORD_ELLIPSIS" />
               </children>
               <VBox.margin>
                  <Insets bottom="6.0" />
               </VBox.margin>
            </VBox>
         </children>
      </VBox>
   </left>
   <center>
      <BorderPane>
         <center>
            <JFXTreeTableView fx:id="jfxListViewOutput" BorderPane.alignment="CENTER">
               <columns>
                  <!--<JFXTreeTableColumn fx:id="jfxColumnTicket" editable="false" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Ticket" />-->
                  <JFXTreeTableColumn fx:id="jfxColumnDuration" editable="false" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Duration" />
                  <JFXTreeTableColumn fx:id="jfxColumnMessage" editable="false" minWidth="133" prefWidth="133" text="Message" />
               </columns>
               <columnResizePolicy>
                  <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </JFXTreeTableView>
         </center>
      </BorderPane>
   </center>
   <bottom>
      <HBox fx:id="jfxContainerCommit" alignment="BOTTOM_CENTER" translateY="70.0" BorderPane.alignment="CENTER">
         <children>
            <JFXTextField fx:id="jfxTextFieldCommit" alignment="BOTTOM_LEFT" focusColor="#5c6bc0" labelFloat="true" promptText="Comment" HBox.hgrow="ALWAYS" VBox.vgrow="NEVER" />
            <JFXButton fx:id="jfxButtonCommit" buttonType="RAISED" prefHeight="40.0" prefWidth="40.0" ripplerFill="WHITE" styleClass="button-default" textOverrun="CLIP">
               <HBox.margin>
                  <Insets bottom="5.0" right="5.0" />
               </HBox.margin>
            </JFXButton>
         </children>
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </HBox>
   </bottom>
</BorderPane>
